<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PhuongNam">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="300dp"
        android:layout_height="167dp"
        android:src="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.07" />

    <ImageView
        android:id="@+id/imgND"
        android:layout_width="110dp"
        android:layout_height="79dp"
        android:src="@drawable/nguoi2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.193"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.088" />

    <TextView
        android:id="@+id/tvND"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Người Dùng"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.216"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgND"
        app:layout_constraintVertical_bias="0.049" />

    <ImageView
        android:id="@+id/imgTheloai"
        android:layout_width="105dp"
        android:layout_height="81dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.594"
        app:layout_constraintStart_toEndOf="@+id/imgND"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.088"
        android:src="@drawable/images"/>

    <TextView
        android:id="@+id/tvTL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Thể Loại"
        app:layout_constraintBottom_toTopOf="@+id/imgSach"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.59"
        app:layout_constraintStart_toEndOf="@+id/tvND"
        app:layout_constraintTop_toBottomOf="@+id/imgTheloai"
        app:layout_constraintVertical_bias="0.253" />

    <ImageView
        android:id="@+id/imgSach"
        android:layout_width="113dp"
        android:layout_height="77dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.194"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgTheloai"
        app:layout_constraintVertical_bias="0.298"
        android:src="@drawable/sach" />

    <ImageView
        android:id="@+id/imgHd"
        android:layout_width="111dp"
        android:layout_height="73dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.612"
        app:layout_constraintStart_toEndOf="@+id/imgSach"
        app:layout_constraintTop_toBottomOf="@+id/tvTL"
        app:layout_constraintVertical_bias="0.198"
        android:src="@drawable/hoadon" />

    <TextView
        android:id="@+id/tvQLS"
        android:layout_width="40dp"
        android:layout_height="22dp"
        android:text="Sách"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.257"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.617" />

    <TextView
        android:id="@+id/tvHĐ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hóa Đơn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.656"
        app:layout_constraintStart_toEndOf="@+id/tvQLS"
        app:layout_constraintTop_toBottomOf="@+id/imgHd"
        app:layout_constraintVertical_bias="0.097" />

    <ImageView
        android:id="@+id/imgSachbanchay"
        android:layout_width="113dp"
        android:layout_height="72dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.194"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgSach"
        app:layout_constraintVertical_bias="0.607"
        android:src="@drawable/thongke" />

    <ImageView
        android:id="@+id/imgTk"
        android:layout_width="108dp"
        android:layout_height="76dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.598"
        app:layout_constraintStart_toEndOf="@+id/imgSachbanchay"
        app:layout_constraintTop_toBottomOf="@+id/tvHĐ"
        app:layout_constraintVertical_bias="0.508"
        android:src="@drawable/thongke2" />

    <TextView
        android:id="@+id/tvSBC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sách Bán Chạy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.218"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.942" />

    <TextView
        android:id="@+id/tvTK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Thống Kê"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.577"
        app:layout_constraintStart_toEndOf="@+id/tvSBC"
        app:layout_constraintTop_toBottomOf="@+id/imgTk"
        app:layout_constraintVertical_bias="0.256" />

</androidx.constraintlayout.widget.ConstraintLayout>